<UserControl x:Class="FilesShelf.App.Controls.BookShelfRow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" d:DesignWidth="623" d:DesignHeight="123" >
    <UserControl.Background>
        <ImageBrush x:Name="BookShelfRow_BackgroundBrush" ImageSource="/FilesShelf.App;component/Images/shelf2.jpg" Stretch="Fill"/>
    </UserControl.Background>
    <Grid x:Name="Shelf_Grid" Width="auto" MinWidth="140" Height="123" >
        <StackPanel  x:Name="Books_Stack" HorizontalAlignment="Center"  Margin="47,10,47,0" Orientation="Horizontal">            
        </StackPanel>
    </Grid>
</UserControl>
