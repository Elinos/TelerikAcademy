<UserControl x:Class="FilesShelf.App.Controls.DescSpaceRow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:FilesShelfControls="clr-namespace:FilesShelf.App.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="130" d:DesignWidth="700">
    <Grid Width="auto" Height="125" MinWidth="700">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="170"/>
        </Grid.ColumnDefinitions>
        <Grid.Background>
            <ImageBrush ImageSource="/FilesShelf.App;component/Images/deskBack.jpg"/>
        </Grid.Background>
        <Rectangle  Grid.ColumnSpan="2" Height="18" VerticalAlignment="Bottom">
            <Rectangle.Fill>
                <ImageBrush ImageSource="/FilesShelf.App;component/Images/desk.gif"/>
            </Rectangle.Fill>
        </Rectangle>        
            <StackPanel x:Name="DeskSpaceBooks_Stack" Orientation="Horizontal" Margin="40 10 15 0">
            </StackPanel>
        <!--<Button Grid.Column="1" Margin="10 10 40 10" Click="Button_Click_1">Open Librarian</Button>-->
    </Grid>
</UserControl>
